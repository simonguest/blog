{
  "name": "blog",
  "version": "1.0.0",
  "description": "blog",
  "dependencies": {},
  "devDependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.137.0",
    "@aws-cdk/aws-cloudfront": "^1.136.0",
    "@aws-cdk/aws-s3": "^1.136.0",
    "@aws-cdk/aws-s3-deployment": "^1.136.0",
    "@aws-cdk/core": "^1.136.0",
    "aws-cdk": "^2.2.0",
    "cypress": "^9.1.0",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.15",
    "parcel": "^2.2.1",
    "parcel-reporter-static-files-copy": "^1.3.2"
  },
  "scripts": {
    "test": "npx cypress run",
    "generate": "node generate.js",
    "prebuild": "npm run generate",
    "build": "npx parcel build index.html",
    "preserver": "npm run build",
    "server": "npx nodemon",
    "predeploy": "npm run build",
    "deploy": "npx cdk deploy --app cdk-deploy.js --require-approval never",
    "undeploy": "npx cdk destroy --app cdk-deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonguest/blog.git"
  },
  "staticFiles": {
    "staticPath": "content",
    "staticOutPath": "content"
  },
  "nodemonConfig": {
    "ext": "js,css,html,md",
    "ignore": ["dist/", ".parcel-cache/", "content/live/index.js", "content/test/index.js"],
    "verbose": true,
    "exec": "npm run build && node server.js"
  },
  "author": "",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/simonguest/blog/issues"
  },
  "homepage": "https://github.com/simonguest/blog#readme"
}
